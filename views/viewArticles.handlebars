 
    <div class="row">      
        <div class="col-md-6">
            <h2>Recent NPR Articles</h2>            
        </div>
    </div>
 
 <hr>

    {{#each article}}
        <div class="row">
            <div class="col-md-6">
                <div id="headline">
                    <p><h4>Headline:</h4></p>
                    <p><h5>{{headline}}</h5></p>
                </div>
                <div id="summary">
                    <p><h4>Summary:</h4></p>
                    <p><h5>{{summary}}</h5></p>
                </div> 
                <div id="link">
                     <p><h4>Article Link:</h4></p>
                    <p><h5>{{articleLink}}</h5></p>
                </div>
                <div id="summary">
                    <p><h4>Website Section:</h4></p>
                    <p><h5>{{websiteSection}}</h5></p>
                </div>            
            </div>
        </div>
    {{/each}}    

<hr id="articleBorder">