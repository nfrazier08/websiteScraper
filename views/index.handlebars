<div class="container">

    <div class="page-header">
        <h1>Have you read the news today?</h1>
    </div>

    <div class="row">      
        <div class="col-md-6">
            <h2>Click to get NPR news articles</h2>
            <button id="scrape" class="btn btn-success">Get NPR articles</button>
        </div>
    </div>

    <hr>

    {{#each newArticle}}
        <div class="row">
            <div class="col-md-6">
                <div id="headline">{{headline}}</div>
                <div id="section"></div>
                <div id="link"></div>
                <div id="summary"></div>            
            </div>
        </div>
    {{/each}}
    

    <hr>



</div>